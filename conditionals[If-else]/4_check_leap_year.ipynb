    "year=int(input(\"enter the year to check leap or not\"))\n",
    "if year%100!=0:        #conditon divide by 4 and not by 100\n",
    "    if year%4==0:\n",
    "        print(\"it is a leap year\")\n",
    "elif year%400==0:\n",
    "    print(\"it is a leap year\")\n",
    "else:\n",
    "    print(\"it is not a leap year\")\n"
